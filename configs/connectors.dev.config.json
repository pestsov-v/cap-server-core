{
  "mongodb": {
    "enable": false,
    "connect": {
      "protocol": "mongodb",
      "host": "localhost",
      "port": 27017
    },
    "auth": {
        "username": "root",
        "password": "root"
        },
    "database": "nisu"
    },
  "typeorm": {
    "enable": true,
    "connect": {
      "dbType": "postgres",
      "protocol": "http",
      "host": "localhost",
      "port": 5432,
      "database": "root",
      "schema": "cargo"
    },
    "auth": {
      "username": "postgres",
      "password": "postgres"
    }
  },
  "redis": {
    "enable": true,
    "connect": {
      "protocol": "redis",
      "host": "localhost",
      "port": 6379
    },
    "options": {
      "retryTimeout": 10000,
      "retryCount": 5,
      "storageMode": "standalone",
      "enableAutoPipelining": true,
      "maxRetriesPerRequest": 10,
      "showFriendlyErrorStack": false
    }
  }
}